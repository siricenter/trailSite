<table class="index-table">
  <thead>
    <tr class="index-th">
      <th>Title</th>
      <th>Traditional Route</th>
      <th>Url</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @traditional_route_videos.each do |traditional_route_video| %>
      <tr class="index-tr">
        <td><%= traditional_route_video.title %></td>
        <td><%= if traditional_route_video.traditional_route != nil then traditional_route_video.traditional_route.name else "traditional route deleted" end %></td>
        <td><iframe class="video" src="<%= traditional_route_video.url %>" frameborder="0" allowfullscreen></iframe></td>
        <td><%= traditional_route_video.description %></td>
        <td class="index-action"><%= link_to 'Show', traditional_route_video %></td>
        <td class="index-action"><%= link_to 'Edit', edit_traditional_route_video_path(traditional_route_video) %></td>
        <td class="index-action"><%= link_to 'Destroy', traditional_route_video, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
